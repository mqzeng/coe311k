    "import numpy as np\n",
    "\n",
    "sum = 0\n",
    "pi = np.pi\n",
    "\n",
    "for i in range(5000):\n",
    "    term = (-1)**i / (2*i + 1)\n",
    "    sum += term\n",
    "    \n",
    "    if (i + 1) % 100 == 0:\n",
    "        estimate = 4 * sum \n",
    "        relError = abs(estimate - pi) / pi\n",
    "        print(estimate, relError)"
   ]
